@defop('sql', 0, 1.5)
def sql(aipl, q:str, db:str):
    import sqlite3
    con = sqlite3.connect(db)
    con.row_factory = sqlite3.Row
    return [
        {k:r[k] for k in r.keys()}
            for r in con.execute(q).fetchall()
    ]


